<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ench="http://schemas.android.com/apk/com.yohnnsafe.ench_wu.yohnnsafe"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.yohnnsafe.ench_wu.yohnnsafe.activity.Setting_Activity"
    >

    <TextView
        style="@style/StyleTitle"
        android:text="设置中心" />

    <com.yohnnsafe.ench_wu.yohnnsafe.view.SettingItemView
        android:id="@+id/siv_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ench:desc_off="自动更新已关闭"
        ench:desc_on="自动更新已开启"
        ench:title_insetting="自动更新设置" />
    <!--<attr name="title" format="string" />-->
    <!--<attr name="desc_on" format="string" />-->
    <!--<attr name="desc_off" format="string" />-->

    <com.yohnnsafe.ench_wu.yohnnsafe.view.SettingItemView
        android:id="@+id/siv_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ench:desc_off="归属地显示已关闭"
        ench:desc_on="归属地显示已开启"
        ench:title_insetting="来电归属地显示设置" />
    <com.yohnnsafe.ench_wu.yohnnsafe.view.SettingClickView
        android:id="@+id/scv_address_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
      />

    <com.yohnnsafe.ench_wu.yohnnsafe.view.SettingClickView
        android:id="@+id/scv_address_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:focusableInTouchMode="true"
        />
    <com.yohnnsafe.ench_wu.yohnnsafe.view.SettingItemView
        android:id="@+id/siv_callsafe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        ench:desc_off="归属地显示已关闭"
        ench:desc_on="归属地显示已开启"
        ench:title_insetting="黑名单拦截设置" />
</LinearLayout>
